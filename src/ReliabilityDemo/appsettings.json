{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "ReliabilityDemo.Services.FailureSimulator": "Trace",
      "ReliabilityDemo.Controllers": "Debug",
      "Microsoft.AspNetCore": "Warning"
    },
    "Console": {
      "IncludeScopes": true,
      "TimestampFormat": "HH:mm:ss "
    }
  },
  "FailureConfig": {
    "ConnectionFailureRate": 0.0,
    "ReadTimeoutRate": 33.0,
    "WriteTimeoutRate": 25.0,
    "SlowResponseRate": 0.0,
    "ReadTimeoutMs": 750,
    "WriteTimeoutMs": 750,
    "SlowResponseDelayMs": 2000
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "Redis": "localhost:6379",
    "SqlServer": "Server=(localdb)\\mssqllocaldb;Database=ReliabilityDemo;Trusted_Connection=true;"
  },
  "DataStore": {
    "Provider": "Redis"
  },
  "RedisDataStore": {
    "MaxConcurrentClients": 5
  },
  "SqlServerDataStore": {
    "MaxConcurrentClients": 5,
    "AutoMigrate": true
  }
}